var timeRange={start:"",end:""};$(document).ready(function(){reBack();setTopUrlCookie("payadmin_listop","财务管理");keyEnter($("#pay_search_btn"));$("tbody tr").mouseover(function(){$(this).css({"background-color":"#f5f5f5"})}).mouseout(function(){$(this).css({"background-color":"#fff"})});var t=new pickerDateRange("SelectData",{isTodayValid:true,defaultText:" ~ ",inputTrigger:"optional",theme:"ta",success:function t(e){timeRange={start:e.startDate,end:e.endDate};updateTime(timeRange)}});$("#SelectRange").on("click","li",function(t){var e=$(this),a=e.data("type"),n=e.text();if(a=="all"){timeRange={start:"",end:""}}else if(a=="nowMonth"){timeRange={start:getNowMonth(),end:getNowDay()}}updateTime(timeRange)});$("#optional").click(function(){$("#dropdown-toggle").dropdown("toggle")});$("#SelectData").off("click").text("全部订单");reBack();updateTime(timeRange)});function updateTime(t){$("#startTime").val(t.start);$("#endTime").val(t.end);if(t.start&&t.start!="2016"){$("#SelectData").text(t.start+" ~ "+t.end)}else{$("#SelectData").text("全部订单")}}function reBack(){var t=GetQueryString("cash_status");if(t){$("#cash_status option[value="+t+"]").prop("selected",true)}var e=GetQueryString("start_time"),a=GetQueryString("end_time");timeRange={start:e||"",end:a||""}}function exportOrderRecords(){$("#ExportModal").modal("show")}function applyWithdrawPage(){window.location.href="/apply_withdraw_page"}function getBindwxPage(){window.location.href="/bind_wx_account_page"}