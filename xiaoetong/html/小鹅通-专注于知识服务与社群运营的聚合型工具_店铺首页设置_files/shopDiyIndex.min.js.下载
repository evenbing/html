	        addClick: function addClick(item, index) {
	            //
	            var type = this.all_module_arr[index];
	            if (item.status !== 1 && (type == 2 || type == 5 || type == 7)) {
	                //                    alert(item.msg);
	                _eventbus2.default.$emit('show_top_prompt_tip', item.msg);
