	                });
	            }
	        },
	        updateAddPart: function updateAddPart(index_sort) {
	            for (var i = 0; i < this.all_module_arr.length; i++) {
	                if (index_sort === this.all_module_arr[i]) {
	                    this.all_module_part.splice(i, 1, {
