function clearNoNum(e,r){e=e.replace(/[^\d.]/g,"");e=e.replace(/^\./g,"");e=e.replace(/\.{2,}/g,".");e=e.replace(".","$#$").replace(/\./g,"").replace("$#$",".");e=e.replace(/^0[\d]+/g,"0");e=e.replace(/^(\-)*(\d+)\.(\d\d).*$/,"$1$2.$3");$(r).val(e)}function onlyNum(e,r){e=e.replace(/[^\d]/g,"");$(r).val(e)}var $formCheck=function(){var e={};e.emptyString=function(e){e=$.trim(e);if(!e||e.length===0){return true}return false};e.checkNum=function(e){e=parseInt(e);if(isNaN(e)||e<=0){return false}return true};e.isNumPositiveAndInteger=function(e){if(isNaN(e)||e<0){return false}if(!/^[1-9][\d]*|0$/.test(e)){return false}return true};e.checkTime=function(e){var r=/^\d{4}-(?:0\d|1[0-2])-(?:[0-2]\d|3[01])( (?:[01]\d|2[0-3])\:[0-5]\d\:[0-5]\d)?$/;if(r.test(e)){return true}return false};e.checkTimeOrder=function(e,r){if(this.checkTime(e)&&this.checkTime(r)){e=new Date(e.replace(/-/g,"/")).getTime();r=new Date(r.replace(/-/g,"/")).getTime();if(r>=e){return true}}return false};e.checkAccount=function(e){if(e.length<6&&e.length>0||e.length>18){return false}var r=/^[0-9a-zA-Z]*$/g;if(!r.test(e)){return false}return true};e.checkPassword=function(e){console.log(e.length);if(e.length<6&&e.length>0||e.length>16){return false}var r=/^[0-9a-zA-Z]*$/g;if(!r.test(e)){return false}return true};e.checkPhone=function(e){var r=/^1[34578]\d{9}$/g;if(!r.test(e)){return false}return true};return e}();